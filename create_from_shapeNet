   "outputs": [],
   "source": [
    "# create/store shapenet voxel data in a .h5 file\n",
    "# Is saving as a Meshes object good? Or should store a something else\n",
    "# maybe a list of Meshes.\n",
    "import os\n",
    "import h5py\n",
    "import trimesh\n",
    "import glob\n",
    "from pytorch3d.io import load_objs_as_meshes, load_obj\n",
    "    \n",
    "def dataset_from_shapeNet(synset_path):\n",
    "    '''Store all the voxel and mesh data of the synset under the path\n",
    "    in a .h5 file.\n",
    "    :args:\n",
    "    synset_path (str): path to an unziped synset directory\n",
    "    '''\n",
    "    # find all files under synset\n",
    "    shape_path_list = glob.glob(os.path.join(synset_path, \"*\"))\n",
    "    shapes = []\n",
    "    mesh_paths = []\n",
    "    \n",
    "    for shape_path in shape_path_list:\n",
    "        # get voxal path\n",
    "#         vox_path = os.path.join(shape_path, \n",
    "#                                 \"models/model_normalized.solid.binvox\")        \n",
    "#         vox = trimesh.load(vox_path)\n",
    "#         vox = vox.revoxelized((32,32,32))\n",
    "#         vox_array = vox.matrix.astype(float)\n",
    "        \n",
    "        # get mesh path\n",
    "        mesh_path = os.path.join(shape_path,\n",
    "                                \"models/model_normalized.obj\")\n",
    "        mesh_paths.append(mesh_path)\n",
    "\n",
    "    meshes = load_objs_as_meshes(mesh_paths, \n",
    "                                 load_textures=False)\n",
    "\n",
    "    # save\n",
    "    file = './shapeNetData/bench_mesh_02828884.h5'\n",
    "    f_train = h5py.File(file, 'w')\n",
    "#     f_train['data'] = shapes\n",
    "    f_train['mesh'] = meshes\n",
    "    f_train.close()"
   ]
  },
